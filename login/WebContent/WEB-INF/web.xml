<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>login.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoggedInServlet</servlet-name>
    <servlet-class>login.LoggedInServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoggedInServlet</servlet-name>
    <url-pattern>/loggedin</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>InsertServlet</servlet-name>
    <servlet-class>signUp.InsertServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>InsertedServlet</servlet-name>
    <servlet-class>signUp.InsertedServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InsertedServlet</servlet-name>
    <url-pattern>/saved</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>InsertServlet</servlet-name>
    <url-pattern>/save</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SearchServlet</servlet-name>
    <servlet-class>search.SearchServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SearchedServlet</servlet-name>
    <servlet-class>search.SearchedServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SearchServlet</servlet-name>
    <url-pattern>/search</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SearchedServlet</servlet-name>
    <url-pattern>/searched</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>favoriteServlet</servlet-name>
    <servlet-class>favorite.favoriteServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>favoritedServlet</servlet-name>
    <servlet-class>favorite.favoritedServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>favoritedServlet</servlet-name>
    <url-pattern>/favorited</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>favoriteServlet</servlet-name>
    <url-pattern>/favorite</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>signup.html</welcome-file>
    <welcome-file>homepage.jsp</welcome-file>
  </welcome-file-list>
</web-app>